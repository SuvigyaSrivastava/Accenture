// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std; 
int result = 0;
int Calculate(int m, int n) {
    for(int i =n; i<=m; i++){
        if(i%3==0 && i%5==0){
            result += i;
        }
    }
    return result; 
}


int main() {
int n;
cin >> n;
int m;
cin>> m;
cout<<Calculate(m,n);
    return 0;
}
